<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
    configVersion="3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://www.phpdoc.org"
    xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd">
    <title>FireHub Web Application Framework</title>
    <paths>
        <output>docs</output>
        <cache>vendor/firehub/documentor/src</cache>
    </paths>
    <version number="3.0.0">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
        </api>
    </version>
    <setting name="app_name" value="Core"/>
    <setting name="branch" value="develop-pre-alpha-m1"/>
    <template name="vendor/firehub/documentor/src/template/api" location=""/>
</phpdocumentor>